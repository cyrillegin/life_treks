{
  "author": "Cyrille Gindreau",
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "@tweenjs/tween.js": "18.3.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "d3": "5.12.0",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "nodemon": "1.19.3",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-ga": "2.7.0",
    "react-router-dom": "5.1.2",
    "react-social-icons": "4.1.0",
    "three": "0.109.0"
  },
  "description": "Personal site",
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/node": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-watch": "^7.0.0",
    "concurrently": "4.1.2",
    "css-loader": "3.2.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "jest": "24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.10.2",
    "sass-loader": "^8.0.0",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "2.1.2",
    "webpack": "4.41.0",
    "webpack-bundle-analyzer": "3.5.2",
    "webpack-cli": "3.3.9"
  },
  "engines": {
    "node": "10.15.0"
  },
  "license": "",
  "name": "",
  "private": true,
  "repository": "",
  "scripts": {
    "build:dev": "webpack --progress --config webpack.dev.config.js src/client",
    "build:prod": "webpack --progress --config webpack.prod.config.js src/client",
    "build:server": "babel src --out-dir dist --copy-files --ignore src/client/ --source-maps inline",
    "build:workers": "babel src/client/sw.js --out-file dist/sw.js --copy-flies",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint src",
    "lint:styles": "sass-lint \"src/**/*.scss\" -v -q",
    "run:prod": "npm run build:prod & npm run start:server'",
    "start": "cp -a src/server/public/. dist/ & concurrently --kill-others \"npm run watch:server\"  \"npm run watch:client\"",
    "test": "npm run prettier && npm run lint && npm run test:unit",
    "prettier": "prettier --list-different \"src/**/*.{js,jsx,scss,md}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,scss,md}\"",
    "test:unit": "jest src",
    "watch:tests": "jest src --watch",
    "watch:server": "babel-watch --watch src/server/ src/server/index.js",
    "watch:client": "webpack --watch --progress --config webpack.dev.config.js src/client"
  },
  "version": "1.0.0",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "src/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "statements": 80,
        "lines": 80,
        "functions": 80
      }
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>utility/setupTests.js"
    ],
    "verbose": false
  }
}
