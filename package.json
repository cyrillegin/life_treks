{
    "name": "life_treks",
    "private": true,
    "version": "1.0.0",
    "description": "Life Treks website",
    "repository": "https://github.com/cyrillegin/life_treks.git",
    "license": "MIT",
    "author": "Cyrille Gindreau",
    "engines": {
        "node": "8.6.2"
    },
    "scripts": {
        "assess": "babel-node node_modules/.bin/code-assess src",
        "start": "npm run build-server && concurrently --kill-others 'npm run build-client' 'npm run start-server'",
        "build-server": "babel src --out-dir dist --copy-files --ignore src/client/",
        "build-client": "webpack --progress --watch --config ./webpack.dev.config.js",
        "start-server": "nodemon --ignore /client/ --exec babel-node src/server",
        "startProd": "npm run build-prod && npm run start-server",
        "build-prod": "webpack --progress --config ./webpack.prod.config.js",
        "lint": "npm run htmllint & npm run scsslint & npm run eslint",
        "htmllint": "./node_modules/.bin/htmlhint --config .htmlhintrc src",
        "scsslint": "./node_modules/.bin/sass-lint src '**/*.scss' -v -q",
        "eslint": "./node_modules/.bin/eslint src",
        "postinstall": "npm --prefix ./node_modules/node-sass/ run install && npm prune",
    },
    "dependencies": {
        "@uirouter/angularjs": "1.0.12",
        "angular": "1.6.8",
        "angular-animate": "1.6.8",
        "angular-aria": "1.6.8",
        "angular-route": "1.6.8",
        "autotrack": "2.4.1",
        "bootstrap": "3.3.7",
        "d3": "4.12.2",
        "font-awesome": "4.7.0",
        "helmet": "3.9.0",
        "jquery": "3.2.1",
        "mongodb": "2.2.3",
        "require": "2.4.20",
        "spin": "0.0.1",
        "three": "0.89.0"
    },
    "devDependencies": {
        "autoprefixer": "7.2.3",
        "babel-cli": "6.26.0",
        "babel-core": "6.26.0",
        "babel-loader": "7.1.2",
        "babel-plugin-angularjs-annotate": "0.8.2",
        "babel-preset-env": "1.6.1",
        "babel-preset-es2015": "6.24.1",
        "body-parser": "1.18.2",
        "css-loader": "0.28.7",
        "code-assess": "0.2.4",
        "concurrently": "3.5.1",
        "dotenv": "4.0.0",
        "exports-loader": "0.6.4",
        "express": "4.16.2",
        "express-session": "1.15.6",
        "extract-text-webpack-plugin": "3.0.2",
        "file-loader": "1.1.6",
        "html-loader": "0.5.1",
        "imports-loader": "0.7.1",
        "less": "2.7.3",
        "less-loader": "4.0.5",
        "node-sass": "4.7.2",
        "nodemailer": "4.4.1",
        "precss": "2.0.0",
        "postcss-loader": "2.0.9",
        "sass-loader": "6.0.6",
        "style-loader": "0.19.1",
        "uglifyjs-webpack-plugin": "0.4.6",
        "url-loader": "0.6.2",
        "webpack": "3.10.0",
        "nodemon": "1.14.3",
        "webpack-dev-server": "2.9.7",
        "webpack-bundle-analyzer": "2.9.1"
    }
}
