  "scripts": {
    "build:dev": "webpack --progress --config webpack.dev.config.js src/client",
    "build:prod": "webpack --progress --config webpack.prod.config.js src/client",
    "build:server": "babel src --out-dir dist --copy-files --ignore src/client/ --source-maps inline",
    "build:workers": "babel src/client/sw.js --out-file dist/sw.js --copy-flies",
    "inc:minor": "python utilities/version.py minor",
    "inc:patch": "python utilities/version.py patch",
    "lint": "npm run lint:js",
    "lint:js": "eslint src",
    "prettier": "prettier --list-different \"src/**/*.{js,md}\"",
    "run:prod": "npm run build:prod & npm run start:server'",
    "start": "npm run start:client & concurrently --kill-others 'npm run start:server'",
    "start:client": "webpack --watch --progress --config webpack.dev.config.js src/client",
    "start:prod": "webpack --watch --progress --config webpack.prod.config.js src/client & concurrently --kill-others 'npm run start:server'",
    "start:server": "nodemon --ignore /client/ --exec babel-node src/server",
    "test": "npm run test:unit && npm run lint",
    "test:unit": "jest src/client"
  },
  "version": "0.1.1"
